container_commands:
  01_migrate:
    command: "source /var/app/venv/*/bin/activate && python3 manage.py migrate"
    leader_only: true
  02_createsu:
    command: "source /var/app/venv/*/bin/activate ; cd /var/app/staging ; python manage.py createsu"
    leader_only: true
  03_collectstatic:
    command: "source /var/app/venv/*/bin/activate ; cd /var/app/staging ; python manage.py collectstatic --noinput"
  04_seed_amenities:
    command: "source /var/app/venv/*/bin/activate && python manage.py seed_amenities"
  05_seed_facilities:
    command: "source /var/app/venv/*/bin/activate && python manage.py seed_facilities"
  06_seed_users:
    command: "source /var/app/venv/*/bin/activate && python manage.py seed_users --number 20"
  07_seed_rooms:
    command: "source /var/app/venv/*/bin/activate && python manage.py seed_rooms --number 30"
option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: config.wsgi:application
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: config.settings